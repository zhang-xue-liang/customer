.index-container .bot{
  padding: 30rpx 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.index-container .bot image{
  width: 75rpx;
  height: 35rpx;
  margin-bottom: 14rpx;
}
.index-container .bot .tousu{
  display: flex;
}
.index-container .bot .tousu view:nth-child(1){
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #999999;
}
.index-container .bot .tousu view:nth-child(2){
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FC7432;
}